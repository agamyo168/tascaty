{
  "expo": {
    "name": "mobile-task-app",
    "slug": "mobile-task-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "mobiletaskapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.mobiletaskapp"
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.anonymous.mobiletaskapp"
    },
    "web": {
      "output": "static"
    },
    "plugins": ["expo-router", ["expo-splash-screen"], "expo-sqlite"],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
